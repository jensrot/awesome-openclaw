  "repository": {
    "type": "git",
    "url": "https://github.com/jensrot/awesome-openclaw.git"
  },
  "keywords": [
    "awesome",
    "awesome-list",
    "openclaw",
    "openclaw-skills"
  ],
  "author": "jensrot",
  "license": "CC0-1.0",
  "devDependencies": {
    "awesome-lint": "^2.2.3",
    "husky": "^9.0.0",
    "remark-cli": "^12.0.1",
    "remark-lint": "^10.0.1",
    "remark-lint-no-dead-urls": "^2.0.1"
  }
}
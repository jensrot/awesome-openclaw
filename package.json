{
  "title": "Awesome OpenClaw",
  "name": "awesome-openclaw",
  "version": "1.0.0",
  "description": "A curated list of OpenClaw tools, skills, and resources",
  "scripts": {
    "prepare": "husky",
    "lint:remote": "awesome-lint https://github.com/jensrot/awesome-openclaw",
    "lint:local": "remark README.md --use remark-lint",
    "lint:urls": "remark README.md --rc-path .remarkrc.json",
    "validate": "bash scripts/validate-readme.sh"
